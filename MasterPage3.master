                            </div>
                            <div class="header_cell">
                                <asp:HyperLink CssClass="element" ID="hlHome" runat="server" Text="Home" ForeColor="White" Font-Underline="false"
                                    Font-Size="X-Large" NavigateUrl="Home.aspx"></asp:HyperLink>
                            </div>
                            <div class="header_cell">
                                <asp:HyperLink CssClass="element" ID="hlContact" runat="server" Text="Contact" ForeColor="White" Font-Underline="false"
                                    Font-Size="X-Large" Style="cursor: pointer;" Visible="False"></asp:HyperLink>
                            </div>
                            <div class="header_cell">
                                <asp:HyperLink CssClass="element" ID="hlPortfolio" runat="server" Text="Projects" Font-Underline="false"
                                    ForeColor="White" Font-Size="X-Large" Style="cursor: pointer;" Visible="False"></asp:HyperLink>
                            </div>
                            <div class="header_cell">
                                <asp:HyperLink CssClass="element" ID="hlExperience" runat="server" Text="Experience" Font-Underline="false"
