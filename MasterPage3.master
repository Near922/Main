                            </div>
                            <div class="header_cell">
                                <asp:HyperLink CssClass="element" ID="hlSkills" runat="server" Text="Skills" ForeColor="White" Font-Underline="false"
                                    Font-Size="X-Large" Style="cursor: pointer;" Visible="False"></asp:HyperLink>
                            </div>
                            <div class="header_cell">
                                <asp:HyperLink CssClass="element" ID="hlAbout" runat="server" Text="About" ForeColor="White" Font-Underline="false"
                                    Font-Size="X-Large" Style="cursor: pointer;" Visible="False"></asp:HyperLink>
                            </div>
                        </div>
                    </div>
                </div>
            <div id="contentWrapper" runat="server" style="height: 100%; min-height: 900px; margin-top: 70px;">
                <asp:ContentPlaceHolder ID="ContentPlaceHolder2" runat="server">
                </asp:ContentPlaceHolder>
                <div class="container">
                    <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
                    </asp:ContentPlaceHolder>

                </div>
                <div style="clear: both;">
                </div>

            </div>
            <div id="divFooter" class="footer" runat="server">
                <div class="container">
                    <div style="padding: 20px; float: left">
                        <div style="padding-right: 10px; float: left;">
                            <asp:ImageButton ID="ImageButton1" runat="server" ImageUrl="images/icons/1444682352_facebook_online_social_media.png"
