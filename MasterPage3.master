    <script src='<%= ResolveUrl("~/Scripts/jquery-2.1.4.js") %>'></script>
    <script src='<%= ResolveUrl("~/Scripts/JScript.js") %>'></script>
    <script src='<%= ResolveUrl("~/Scripts/jQuery-Knob-1.2.13/js/jquery.knob.js") %>'></script>
    <script src='<%= ResolveUrl("~/Scripts/jquery-ui.min.js") %>'></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.js"></script>
